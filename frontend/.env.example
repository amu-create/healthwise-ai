# API 엔드포인트
REACT_APP_API_URL=http://localhost:8000/api

# WebSocket 엔드포인트
REACT_APP_WS_URL=ws://localhost:8000/ws

# 앱 이름
REACT_APP_NAME=HealthWise

# 버전
REACT_APP_VERSION=0.1.0

# 환경
REACT_APP_ENV=development

# 기능 플래그
REACT_APP_ENABLE_SOCIAL=true
REACT_APP_ENABLE_POSE_ANALYSIS=true
REACT_APP_ENABLE_ACHIEVEMENTS=true

# 외부 API (필요시 추가)
# REACT_APP_GOOGLE_MAPS_KEY=your-key-here
# REACT_APP_FIREBASE_CONFIG=your-config-here
